    <string name="prefs_battery_saver_title">Белый список</string>
    <string name="prefs_battery_saver_summary">Белый список</string>

    <string name="cached_apps_freezer_title">Заморозка кэшированных приложений</string>
    <string name="cached_apps_freezer_summary">Включает режим заморозки кэшированных приложений</string>

    <string name="extreme_freezer_title">Экстремальный режим фризера</string>
    <string name="extreme_freezer_summary">Замораживает фоновые приложения с активными сервисами</string>

    <string name="pinned_freezer_title">Замораживать закрепленные</string>
    <string name="pinned_freezer_summary">Позволяет замораживать неактивные закрепленные приложения</string>

    <string name="gms_freezer_title">Замораживать сервисы Гугл</string>
    <string name="gms_freezer_summary">Позволяет замораживать некоторые сервисы Гугл</string>

    <string name="keep_freezed_title">Удерживать замороженные</string>
    <string name="keep_freezed_summary">Не закрывать замороженные фоновые приложения по таймауту</string>

