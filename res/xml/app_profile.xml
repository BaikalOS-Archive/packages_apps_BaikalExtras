            android:key="app_profile_phka"
            android:title="@string/app_profile_phka_title"
            android:summary="@string/app_profile_phka_summary"
            android:persistent="false" />


        <SwitchPreference
            android:key="app_profile_reader"
            android:title="@string/app_profile_reader_title"
            android:summary="@string/app_profile_reader_summary"
            android:persistent="false" />

        <SwitchPreference
            android:key="app_profile_stamina"
            android:title="@string/app_profile_stamina_title"
            android:summary="@string/app_profile_stamina_summary"
            android:defaultValue="false" />

        <SwitchPreference
            android:key="app_profile_require_gms"
            android:title="@string/app_profile_require_gms_title"
            android:summary="@string/app_profile_require_gms_summary"
            android:defaultValue="false" />

        <com.aicp.gear.preference.SeekBarPreferenceCham
            android:key="app_profile_volume_scale"
            android:title="@string/app_profile_volume_scale_title"
            android:max="100"
            android:min="-1"
            aicpextras:unitsLeft=""
            aicpextras:unitsRight=""
            android:defaultValue="-1"/>


        <SwitchPreference
            android:key="app_profile_block_focus_recv"
            android:title="@string/app_profile_block_focus_recv_title"
            android:summary="@string/app_profile_block_focus_recv_summary"
            android:defaultValue="false" />

        <SwitchPreference
            android:key="app_profile_block_focus_send"
            android:title="@string/app_profile_block_focus_send_title"
            android:summary="@string/app_profile_block_focus_send_summary"
            android:defaultValue="false" />

        <SwitchPreference
            android:key="app_profile_force_sonification"
            android:title="@string/app_profile_force_sonification_title"
            android:summary="@string/app_profile_force_sonification_summary"
            android:defaultValue="false" />


