<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aicpextras="http://schemas.android.com/apk/res-auto"
    android:title="@string/app_profile_title">

        <SwitchPreference
            android:key="app_profile_pinned"
            android:title="@string/app_profile_pinned_title"
            android:summary="@string/app_profile_pinned_summary"
            android:defaultValue="false" />

        <SwitchPreference
            android:key="app_profile_disable_boot"
            android:title="@string/app_profile_disable_title"
            android:summary="@string/app_profile_disable_summary"
            android:defaultValue="false" />

        <ListPreference
            android:key="app_profile_background"
            android:title="@string/app_profile_background_title"
            android:entries="@array/background_restrictions_entries"
            android:entryValues="@array/background_restrictions_values" 
            android:defaultValue="default"
            android:summary="%s"/>

        <ListPreference
            android:key="app_profile_performance"
            android:title="@string/app_profile_performance_title"
            android:entries="@array/performance_listentries"
            android:entryValues="@array/performance_listvalues" 
            android:defaultValue="default"
            android:summary="%s"/>

        <ListPreference
            android:key="app_profile_thermal"
            android:title="@string/app_profile_thermal_title"
            android:entries="@array/thermal_listentries"
            android:entryValues="@array/thermal_listvalues"
            android:defaultValue="default"
            android:summary="%s" />

        <ListPreference
            android:key="app_profile_brightness"
            android:title="@string/app_profile_brightness_title"
            android:entries="@array/brightness_listentries"
            android:entryValues="@array/brightness_listvalues"
            android:defaultValue="default"
            android:summary="%s" />

        <ListPreference
            android:key="app_profile_rotation"
            android:title="@string/app_profile_rotation_title"
            android:entries="@array/rotation_listentries"
            android:entryValues="@array/rotation_listvalues"
            android:defaultValue="default"
            android:summary="%s" />

        <ListPreference
            android:key="app_profile_fps"
            android:title="@string/app_profile_fps_title"
            android:entries="@array/fps_listentries"
            android:entryValues="@array/fps_listvalues"
            android:defaultValue="default"
            android:summary="%s" />

        <SwitchPreference
            android:key="app_profile_keep_on"
            android:title="@string/app_profile_keep_on_title"
            android:summary="@string/app_profile_keep_on_summary"
            android:defaultValue="false" />

        <ListPreference
            android:key="app_profile_location"
            android:title="@string/app_profile_location_title"
            android:entries="@array/location_listentries"
            android:entryValues="@array/location_listvalues"
            android:defaultValue="0"
            android:summary="%s" />


        <ListPreference
            android:key="app_profile_spoof"
            android:title="@string/app_profile_spoof_title"
            android:entries="@array/spoof_listentries"
            android:entryValues="@array/spoof_listvalues"
            android:defaultValue="default"
            android:summary="%s" />

